
<% layout('layouts/boilerplate')%>
    <div class="text-center bg-info">
        <h2>NUEVA EMPRESA</h2>
    </div>
    <div class="row" >
        <form class="col-md-10 offset-md-1" action="/empresas/nuevaempresa" method="POST">
           
            
            <div class="row">
                <div class="form-group col-md-4">
                    <label for ="empresa" > Empresa </label>
                    <input type="text" class="form-control" id="nombreEmp" name="EMPRESA" placeholder="Nombre Empresa">
                </div>
                <div class="form-group col-md-4">
                    <label for ="nombre" > Contacto </label>
                    <input type="text" class="form-control"  id="nombreResp" name="CONTACTO" placeholder="Nombre Responsable">
                </div>                        
                <div class="form-group col-md-4">
                    <label for ="email" > Email </label>
                    <input type="text" class="form-control" id="emailResp" placeholder="Email Responsable" name="EMAIL">
                </div>
            </div>
           <div class="row">
                <div class="form-group col-md-4">
                    <label for ="telefono" > Telefono </label>
                    <input type="text" class="form-control" id="telefonResp" placeholder="Telefono Responsable" name="TELEFONO">
                </div>
            
                <div class="form-group col-md-4">
                    <label for ="practicas"> Practicas </label>
                    <input type="text" id="practicas" class="form-control" name="PRACTICAS" placeholder="Tipo de PrÃ¡cticas">
                </div>
            
           </div>
           <div class="row mb-3">
            <label for="observaciones">OBSERVACIONES</label>
            <textarea name="OBSERVACIONES" id="observaciones" cols="30" rows="10"></textarea>
          </div>
            
           <div  class="btn col-md-12">
            <button  class="btn btn-outline-success" > Agregar Empresa</button>
        </div>
            
        </form>

       

    </div>

    